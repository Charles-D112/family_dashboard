<div class="container w-50 mt-5">
<h1><%= "Classement" %></h1>

  <div>
    <% @users.each do |user| %>
      <div>
        <%= user.name %>
          <ul>
            <% user.my_executed_tasks.each do |task| %>
              <li><%= task.name %> : 1 point </li>
            <% end %>
          </ul>
      </div>
    <% end %>
  </div>

</div>
